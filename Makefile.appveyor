CPP = C:/MinGW/bin/g++.exe
RM = rd /s /q
MKDIR = mkdir

all: build

build:
	$(RM) C:\bsscript\bin\
	$(MKDIR) C:\bsscript\bin\obj\crypto\
	$(MKDIR) C:\bsscript\bin\obj\misc\
	$(CPP) -c C:\bsscript\lib\main.cpp -o C:\bsscript\bin\obj\main.o
	$(CPP) -c C:\bsscript\lib\crypto\sha1\sha1.cpp -o C:\bsscript\bin\obj\crypto\sha1.o
	$(CPP) -c C:\bsscript\lib\crypto\sha2\sha2.cpp -o C:\bsscript\bin\obj\crypto\sha2.o
	$(CPP) -c C:\bsscript\lib\crypto\sha3\sha3.cpp -o C:\bsscript\bin\obj\crypto\sha3.o
	$(CPP) -o C:\bsscript\bin\bss.exe C:\bsscript\bin\obj\main.o C:\bsscript\bin\obj\crypto\sha1.o C:\bsscript\bin\obj\crypto\sha2.o C:\bsscript\bin\obj\crypto\sha3.o
